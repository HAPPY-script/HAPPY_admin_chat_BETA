local Players = game:GetService("Players")
local player = Players.LocalPlayer
local StarterGui = game:GetService("StarterGui")


local function sendNotification()
    StarterGui:SetCore("SendNotification", {
        Title = "ðŸ””Chat ;H_menu_tad or", 
        Text = "Chat ;H_menu for instructionsðŸ“Œ",
        Duration = 30
    })
end


player.CharacterAdded:Connect(function()
    wait(1)  
    sendNotification()
end)

--[[hbox
local bloxFruitsGameIds = {2753915549, 4442272183, 7449423635}

local function isBloxFruitsGame()
    for _, gameId in ipairs(bloxFruitsGameIds) do
        if game.PlaceId == gameId then
            return true
        end
    end
    return false
end

if isBloxFruitsGame() then
    _G.HeadSize = 2
    _G.Disabled = true

    local userInputService = game:GetService("UserInputService")
    local isHoldingX = false
    local sizeResetTimer = nil

    game:GetService('RunService').RenderStepped:connect(function()
        if _G.Disabled then
            for i, v in next, game:GetService('Players'):GetPlayers() do
                if v.Name ~= game:GetService('Players').LocalPlayer.Name then
                    pcall(function()
                        v.Character.HumanoidRootPart.Size = Vector3.new(_G.HeadSize, _G.HeadSize, _G.HeadSize)
                        v.Character.HumanoidRootPart.Transparency = 1
                        v.Character.HumanoidRootPart.BrickColor = BrickColor.new("Really white")
                        v.Character.HumanoidRootPart.Material = "Neon"
                        v.Character.HumanoidRootPart.CanCollide = false
                    end)
                end
            end
        end
    end)

    userInputService.InputBegan:Connect(function(input)
        if input.KeyCode == Enum.KeyCode.X then
            isHoldingX = true
            wait(1)
            if isHoldingX then
                _G.HeadSize = 77
            end
        end
    end)

    userInputService.InputEnded:Connect(function(input)
        if input.KeyCode == Enum.KeyCode.X then
            isHoldingX = false
            if not sizeResetTimer then
                sizeResetTimer = true
                wait(1)
                _G.HeadSize = 2
                wait(1)
                sizeResetTimer = nil
            end
        end
    end)
else
    warn("Script chá»‰ hoáº¡t Ä‘á»™ng trong game Blox Fruits.")
end
]]
